{
    "format_version": "$general.format_version",
    "animations": {
        "@meta": { "validate": "validateIDs.js" },
        "$placeholder": {
            "animation_length": "$general.number",
            "loop": "$general.boolean",
            "timeline": {
                "$placeholder": {
                    "$dynamic.list.next_index": {
                        "@meta": { "is_value": true },
                        "@import.value.action": "('@s ' + $dynamic.entity.all_events) and ('/function ' + $dynamic.function_files) and ('/' + $function.main)"
                    }
                }
            }
        }
    }
}